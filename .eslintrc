{
    "extends": ["react-app", "react-app/jest"],
    "rules": {
        "no-unused-vars": "warn",
        "prefer-const": "warn",
        "no-console": "warn",
        "quotes": ["warn", "single"],
        "jsx-quotes": ["warn", "prefer-double"],
        "indent": ["warn", 4],
        "max-len": ["error", {"code": 120}],
        "semi": ["warn", "always"]
    }
}
